        <script>
        PNotify.prototype.options.styling = "jqueryui";
        function portal_check_pnotify() {
            $PBJQ.getJSON( "/direct/portal/notify.json", function( data ) {
                for(i=0; i<data.error.length; i++ ) {
                     $PBJQ(function(){
                        new PNotify({
                            title: 'Notice',
                            text: data.error[i],
                            type: 'notice'
                        });
                    });
                }
            });
        }
            </script>

